{
  "version": 6,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 25,
    "patch": 0
  },
  "configurePresets": [
    {
      "name": "default",
      "displayName": "Default Config",
      "description": "Default build using Ninja generator",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/Build/Default"
    },
    {
      "name": "arm-only",
      "inherits": "default",
      "displayName": "arm-only configuration",
      "description": "This build is only available on ARM",
      "cacheVariables": {
        "CMAKE_SYSTEM_NAME": "Generic",
        "CMAKE_SYSTEM_VERSION": "1",
        "CMAKE_C_COMPILER": "C:/Users/treys/OneDrive/Downloads/Compilers/ARM-GCC-12.2-REL1/bin/arm-none-eabi-gcc.exe",
        "CMAKE_CXX_COMPILER": "C:/Users/treys/OneDrive/Downloads/Compilers/ARM-GCC-12.2-REL1/bin/arm-none-eabi-g++.exe",
        "CMAKE_ASM_COMPILER": "C:/Users/treys/OneDrive/Downloads/Compilers/ARM-GCC-12.2-REL1/bin/arm-none-eabi-gcc.exe",
        "CMAKE_AR": "C:/Users/treys/OneDrive/Downloads/Compilers/ARM-GCC-12.2-REL1/bin/arm-none-eabi-ar.exe",
        "CMAKE_OBJCOPY": "C:/Users/treys/OneDrive/Downloads/Compilers/ARM-GCC-12.2-REL1/bin/arm-none-eabi-objcopy.exe",
        "CMAKE_OBJDUMP": "C:/Users/treys/OneDrive/Downloads/Compilers/ARM-GCC-12.2-REL1/bin/arm-none-eabi-objdump.exe",
        "SIZE": "C:/Users/treys/OneDrive/Downloads/Compilers/ARM-GCC-12.2-REL1/bin/arm-none-eabi-size",
        "CMAKE_TRY_COMPILE_TARGET_TYPE": "STATIC_LIBRARY",
        "PLATFORM": "arm"
      },
      "binaryDir": "${sourceDir}/Build/ARM",
      "vendor": {
        "jetbrains.com/clion": {
          "toolchain": "ARM GCC 12.2"
        }
      }
    },
    {
      "name": "windows-only",
      "inherits": "default",
      "displayName": "Windows-only configuration",
      "description": "This build is only available on Windows",
      "cacheVariables": {
        "PLATFORM": "win",
        "UNIT_TESTING": true
      },
      "binaryDir": "${sourceDir}/Build/WIN",
      "vendor": {
        "jetbrains.com/clion": {
          "toolchain": "MinGW 13.1"
        }
      }
    }
  ]
}